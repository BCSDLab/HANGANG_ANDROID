<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:isScrollContainer="true">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/lecture_evaluation_nestedscrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <in.hangang.core.view.appbar.BaseAppBar
                android:id="@+id/lecture_evaluation_appbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:titleText="@string/title_evaluation" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_appbar">
                <TextView
                    android:id="@+id/lecture_evaluation_semester_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:text="@string/semester_title"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16dp"
                    />
                <in.hangang.core.view.spinner.BottomSheetSpinner
                    android:id="@+id/lecture_evaluation_semester_spinner"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_semester_title"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="16dp"/>
                <TextView
                    android:id="@+id/lecture_evaluation_attendance_frequency_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:text="@string/attendance_frequency"
                    android:layout_marginTop="42dp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_semester_spinner"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:text="출첵빈도"
                    />
                <RadioGroup
                    android:id="@+id/lecture_evaluation_attendance_frequency_radiogroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_attendance_frequency_title"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal"
                    android:layout_marginStart="16dp">
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_attendance_frequency_high"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:text="@string/high"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_attendance_frequency_middle"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:layout_marginStart="8dp"
                        android:text="@string/middle"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_attendance_frequency_low"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:layout_marginStart="8dp"
                        android:textColor="@color/black2"
                        android:text="@string/low"/>
                </RadioGroup>
                <TextView
                    android:id="@+id/lecture_evaluation_assignment_info_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:text="@string/assignment_info"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_attendance_frequency_radiogroup"
                    android:layout_marginTop="37dp"
                    android:layout_marginStart="16dp"
                    tools:text="과제정보"
                    />
                <TextView
                    android:id="@+id/lecture_evaluation_assignment_info_guide"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:textSize="12sp"
                    android:textColor="@color/gray_500"
                    android:text="@string/duplicated_select"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_assignment_info_title"
                    android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/lecture_evaluation_assignment_info_title"
                    tools:text="중복선택"
                    />
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_assignment_info_1"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:textSize="14sp"
                    android:textColor="@color/black2"
                    android:text="@string/team_play"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_assignment_info_title"
                    app:layout_constraintStart_toStartOf="parent"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_assignment_info_2"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:textSize="14sp"
                    android:textColor="@color/black2"
                    android:text="@string/assignment_report"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_assignment_info_1"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_assignment_info_1"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_assignment_info_3"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:textSize="14sp"
                    android:textColor="@color/black2"
                    android:text="@string/debate"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_assignment_info_1"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_assignment_info_2"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_assignment_info_4"
                    android:layout_width="64dp"
                    android:layout_height="32dp"
                    android:textSize="14sp"
                    android:textColor="@color/black2"
                    android:text="@string/quize"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_assignment_info_1"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_assignment_info_3"/>
                <TextView
                    android:id="@+id/lecture_evaluation_homework_size_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:text="@string/homework_size"
                    android:layout_marginTop="37dp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_assignment_info_1"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:text="과제량" />
                <RadioGroup
                    android:id="@+id/lecture_evaluation_homework_radiogroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_homework_size_title"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal"
                    android:layout_marginStart="16dp">
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_homework_high"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:text="@string/high"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_homework_middle"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:layout_marginStart="8dp"
                        android:text="@string/middle"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_homework_low"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:layout_marginStart="8dp"
                        android:textColor="@color/black2"
                        android:text="@string/low"/>
                </RadioGroup>
                <TextView
                    android:id="@+id/lecture_evaluation_exam_difficulty_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textColor="@color/black2"
                    android:textSize="16sp"
                    android:layout_marginTop="32dp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_homework_radiogroup"
                    android:text="@string/exam_difficulty"/>
                <RadioGroup
                    android:id="@+id/lecture_evaluation_exam_difficulty_radiogroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_exam_difficulty_title"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal"
                    android:layout_marginStart="16dp">
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_exam_difficulty_high"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:text="@string/high"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_exam_difficulty_middle"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:layout_marginStart="8dp"
                        android:text="@string/middle"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_exam_difficulty_low"
                        android:layout_width="64dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:layout_marginStart="8dp"
                        android:textColor="@color/black2"
                        android:text="@string/low"/>
                </RadioGroup>
                <TextView
                    android:id="@+id/lecture_evaluation_grade_portion_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:text="@string/grade_ratio"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_exam_difficulty_radiogroup"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="37dp"
                    android:layout_marginStart="16dp"
                    tools:text="성적비율"
                    />
                <RadioGroup
                    android:id="@+id/lecture_evaluation_grade_portion_radiogroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_grade_portion_title"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal"
                    android:layout_marginStart="16dp">
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_grade_portion_high"
                        android:layout_width="104dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:text="@string/grade_portion_high"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_grade_portion_middle"
                        android:layout_width="104dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:textColor="@color/black2"
                        android:layout_marginStart="8dp"
                        android:text="@string/grade_portion_middle"/>
                    <in.hangang.core.view.button.radiobutton.FilledRadioButton
                        android:id="@+id/lecture_evaluation_grade_portion_low"
                        android:layout_width="104dp"
                        android:layout_height="32dp"
                        android:textSize="14sp"
                        android:layout_marginStart="8dp"
                        android:textColor="@color/black2"
                        android:text="@string/grade_portion_low"/>
                </RadioGroup>
                <TextView
                    android:id="@+id/lecture_evaluation_hashtag_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:text="@string/hash_tag"
                    android:layout_marginTop="37dp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_grade_portion_radiogroup"
                    tools:text="해시태그" />
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_1"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="16dp"
                    android:text="@string/hash_tag1"
                    android:textSize="14sp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/lecture_evaluation_hash_tag_type_2"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_hashtag_title"/>

                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_2"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:text="@string/hash_tag2"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_hash_tag_type_1"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_hash_tag_type_1"
                    app:layout_constraintEnd_toStartOf="@+id/lecture_evaluation_hash_tag_type_3"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_3"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:text="@string/hash_tag3"
                    android:textSize="14sp"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_hash_tag_type_2"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_hash_tag_type_1"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_4"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/hash_tag4"
                    android:textSize="14sp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/lecture_evaluation_hash_tag_type_5"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_hash_tag_type_1"/>

                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_5"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:text="@string/hash_tag5"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_hash_tag_type_4"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_hash_tag_type_4"
                    app:layout_constraintEnd_toStartOf="@+id/lecture_evaluation_hash_tag_type_6"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_6"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:text="@string/hash_tag6"
                    android:textSize="14sp"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_hash_tag_type_5"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_hash_tag_type_4"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_7"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/hash_tag7"
                    android:textSize="14sp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/lecture_evaluation_hash_tag_type_8"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_hash_tag_type_4"/>

                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_8"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:text="@string/hash_tag8"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_hash_tag_type_7"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_hash_tag_type_7"
                    app:layout_constraintEnd_toStartOf="@+id/lecture_evaluation_hash_tag_type_9"/>
                <in.hangang.core.view.button.checkbox.FilledCheckBox
                    android:id="@+id/lecture_evaluation_hash_tag_type_9"
                    android:layout_width="104dp"
                    android:layout_height="32dp"
                    android:text="@string/hash_tag9"
                    android:textSize="14sp"
                    android:layout_marginEnd="16dp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_hash_tag_type_8"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintTop_toTopOf="@+id/lecture_evaluation_hash_tag_type_7"
                    app:layout_constraintEnd_toEndOf="parent"/>
                <TextView
                    android:id="@+id/lecture_evaluation_total_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/black2"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="37dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_hash_tag_type_7"
                    tools:text="총평"
                    android:text="@string/total_evaluation"
                    />
                <ImageView
                    android:id="@+id/lecture_evaluation_exclamation_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_exclamation"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_total_title"
                    app:layout_constraintBottom_toBottomOf="@+id/lecture_evaluation_total_title"
                    android:visibility="visible"/>
                <TextView
                    android:id="@+id/lecture_evaluation_exclamation_message"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/noti_uncompleted_review"
                    app:layout_constraintStart_toEndOf="@+id/lecture_evaluation_exclamation_icon"
                    app:layout_constraintBottom_toBottomOf="@+id/lecture_evaluation_exclamation_icon"
                    android:textSize="11sp"
                    android:textColor="@color/mango"
                    android:layout_marginStart="4dp"
                    android:visibility="visible"/>
                <androidx.appcompat.widget.AppCompatRatingBar
                    android:id="@+id/lecture_evaluation_rating_bar"
                    style="?android:attr/ratingBarStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:numStars="5"
                    android:rating="0.5"
                    android:stepSize="0.5"
                    android:max="5"
                    android:isIndicator="false"
                    android:progressTint="@color/mango"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_total_title"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="16dp"/>
                <View
                    android:id="@+id/lecture_detail_comment_background_view"
                    android:layout_width="0dp"
                    android:layout_height="152dp"
                    android:background="@drawable/rectangle_rounded_corner_gray_300"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginHorizontal="16dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_evaluation_rating_bar" />
                <EditText
                    android:id="@+id/lecture_evaluation_comment"
                    android:layout_width="0dp"
                    android:gravity="top"
                    android:layout_height="0dp"
                    android:textColor="@color/black2"
                    app:layout_constraintStart_toStartOf="@+id/lecture_detail_comment_background_view"
                    app:layout_constraintEnd_toEndOf="@+id/lecture_detail_comment_background_view"
                    app:layout_constraintTop_toTopOf="@+id/lecture_detail_comment_background_view"
                    app:layout_constraintBottom_toBottomOf="@+id/lecture_detail_comment_background_view"
                    android:background="@android:color/transparent"
                    android:textSize="14sp"
                    android:hint="@string/lecture_evaluation_write_guide_message"
                    android:layout_margin="16dp"/>

                <in.hangang.core.view.button.RoundedCornerButton
                    android:id="@+id/lecture_evaluation_complete_button"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_marginBottom="32dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginTop="24dp"
                    app:layout_constraintTop_toBottomOf="@+id/lecture_detail_comment_background_view"
                    android:backgroundTint="@color/color_state_button_blue"
                    android:text="@string/lecture_evaluation_write_complete"
                    android:gravity="center"
                    android:enabled="false"
                    android:textSize="14sp"
                    android:textColor="@color/white"/>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</layout>